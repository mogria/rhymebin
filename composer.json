{
    "name": "mogria/rhymebin",
    "description": "A Website for finding rhymes",
    "keywords": ["website", "lumen", "rhymes", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": ">=2.4",
        "tymon/jwt-auth": "^1.0@dev",
        "laravelista/lumen-vendor-publish": "^1.0",
        "jubayed/lumen-commands": "dev-master",
        "eloquent/lcs": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "deployer/deployer": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
